<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_layout"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/guideFragment"
        android:name="com.example.pchub.Fragment.GuideFragment"
        android:label="GuideFragment" >
        <action
            android:id="@+id/action_guideFragment_to_assembly"
            app:destination="@id/assembly" />
        <action
            android:id="@+id/action_guideFragment_to_recommendations"
            app:destination="@id/recommendations" />
    </fragment>
    <fragment
        android:id="@+id/userFragment"
        android:name="com.example.pchub.Fragment.UserFragment"
        android:label="UserFragment" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.pchub.Fragment.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_psu2"
            app:destination="@id/psu2" />
        <action
            android:id="@+id/action_homeFragment_to_motherboard2"
            app:destination="@id/motherboard2" />
        <action
            android:id="@+id/action_homeFragment_to_cpu2"
            app:destination="@id/cpu2" />
        <action
            android:id="@+id/action_homeFragment_to_graphics2"
            app:destination="@id/graphics2" />
        <action
            android:id="@+id/action_homeFragment_to_ssd2"
            app:destination="@id/ssd" />
    </fragment>
    <fragment
        android:id="@+id/cpu2"
        android:name="com.example.pchub.Fragment.cpu"
        android:label="fragment_cpu"
        tools:layout="@layout/fragment_cpu" />
    <fragment
        android:id="@+id/graphics2"
        android:name="com.example.pchub.Fragment.graphics"
        android:label="fragment_graphics"
        tools:layout="@layout/fragment_graphics" />
    <fragment
        android:id="@+id/psu2"
        android:name="com.example.pchub.Fragment.psu"
        android:label="fragment_psu"
        tools:layout="@layout/fragment_psu" />
    <fragment
        android:id="@+id/motherboard2"
        android:name="com.example.pchub.Fragment.motherboard"
        android:label="fragment_motherboard"
        tools:layout="@layout/fragment_motherboard" />
    <fragment
        android:id="@+id/ssd"
        android:name="com.example.pchub.Fragment.ssd"
        android:label="fragment_ssd"
        tools:layout="@layout/fragment_ssd" />
    <activity
        android:id="@+id/frontPage"
        android:name="com.example.pchub.Activities.FrontPage"
        android:label="activity_front_page"
        tools:layout="@layout/activity_front_page" />
    <activity
        android:id="@+id/startup"
        android:name="com.example.pchub.Activities.Startup"
        android:label="activity_startup"
        tools:layout="@layout/activity_startup" />
    <activity
        android:id="@+id/login"
        android:name="com.example.pchub.Activities.Login"
        android:label="activity_login"
        tools:layout="@layout/activity_login" />
    <activity
        android:id="@+id/register"
        android:name="com.example.pchub.Activities.Register"
        android:label="Register" />
    <fragment
        android:id="@+id/assembly"
        android:name="com.example.pchub.Fragment.assembly"
        android:label="assembly" >
        <action
            android:id="@+id/action_assembly_to_assemstep1"
            app:destination="@id/assemstep1" />
        <action
            android:id="@+id/action_assembly_to_assemstep2"
            app:destination="@id/assemstep2" />
        <action
            android:id="@+id/action_assembly_to_assemstep3"
            app:destination="@id/assemstep3" />
        <action
            android:id="@+id/action_assembly_to_assemstep4"
            app:destination="@id/assemstep4" />
        <action
            android:id="@+id/action_assembly_to_assemstep5"
            app:destination="@id/assemstep5" />
        <action
            android:id="@+id/action_assembly_to_assemstep6"
            app:destination="@id/assemstep6" />
        <action
            android:id="@+id/action_assembly_to_assemstep7"
            app:destination="@id/assemstep7" />
    </fragment>
    <fragment
        android:id="@+id/recommendations"
        android:name="com.example.pchub.Fragment.recommendations"
        android:label="recommendations" />
    <fragment
        android:id="@+id/assemstep1"
        android:name="com.example.pchub.Fragment.assemstep1"
        android:label="assemstep1" />
    <fragment
        android:id="@+id/assemstep2"
        android:name="com.example.pchub.Fragment.assemstep2"
        android:label="assemstep2" />
    <fragment
        android:id="@+id/assemstep3"
        android:name="com.example.pchub.Fragment.assemstep3"
        android:label="assemstep3" />
    <fragment
        android:id="@+id/assemstep4"
        android:name="com.example.pchub.Fragment.assemstep4"
        android:label="assemstep4" />
    <fragment
        android:id="@+id/assemstep5"
        android:name="com.example.pchub.Fragment.assemstep5"
        android:label="assemstep5" />
    <fragment
        android:id="@+id/assemstep6"
        android:name="com.example.pchub.Fragment.assemstep6"
        android:label="assemstep6" />
    <fragment
        android:id="@+id/assemstep7"
        android:name="com.example.pchub.Fragment.assemstep7"
        android:label="assemstep7" />
</navigation>